DB_HOST=localhost             # or IP address of the database server
DB_PORT=5432                  # default port for PostgreSQL, adjust for your DB (e.g., 3306 for MySQL)
DB_USER=your_database_user    # database username
DB_PASSWORD=your_password     # database password
DB_NAME=your_database_name    # name of the database
PORT=3000                     # port for running the application (e.g., 3000)